{"ast":null,"code":"import GoodsItem from '@/components/GoodsItem.vue';\nimport { getHomeInfo } from '@/api/home';\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      bannerList: [],\n      navList: [],\n      proList: []\n    };\n  },\n  components: {\n    GoodsItem\n  },\n  async created() {\n    const {\n      data: {\n        pageData\n      }\n    } = await getHomeInfo();\n    this.bannerList = pageData.items[1].data;\n    this.navList = pageData.items[3].data;\n    this.proList = pageData.items[6].data;\n    console.log(pageData);\n  }\n};","map":{"version":3,"names":["GoodsItem","getHomeInfo","name","data","bannerList","navList","proList","components","created","pageData","items","console","log"],"sources":["src/views/layout/home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <!-- 导航条 -->\r\n    <van-nav-bar title=\"智慧商城\" fixed />\r\n\r\n    <!-- 搜索框 -->\r\n    <van-search\r\n      readonly\r\n      shape=\"round\"\r\n      background=\"#f1f1f2\"\r\n      placeholder=\"请在此输入搜索关键词\"\r\n      @click=\"$router.push('/search')\"\r\n    />\r\n\r\n    <!-- 轮播图 -->\r\n    <van-swipe class=\"my-swipe\" :autoplay=\"3000\" indicator-color=\"white\">\r\n      <van-swipe-item\r\n      v-for=\"item in  bannerList\" :key=\"item.imgUrl\r\n\">\r\n        <img :src=\"item.imgUrl\" alt=\"\">\r\n      </van-swipe-item>\r\n    </van-swipe>\r\n\r\n    <!-- 导航 -->\r\n    <van-grid column-num=\"5\" icon-size=\"40\">\r\n      <van-grid-item\r\n      v-for=\"item in navList\" :key=\"item.imgUrl\"\r\n        :icon=\"item.imgUrl\"\r\n        :text=\"item.text\"\r\n        @click=\"$router.push('/category')\"\r\n      />\r\n    </van-grid>\r\n\r\n    <!-- 主会场 -->\r\n    <div class=\"main\">\r\n      <img src=\"@/assets/main.png\" alt=\"\">\r\n    </div>\r\n\r\n    <!-- 猜你喜欢 -->\r\n    <div class=\"guess\">\r\n      <p class=\"guess-title\">—— 猜你喜欢 ——</p>\r\n\r\n      <div class=\"goods-list\">\r\n        <GoodsItem v-for=\"item in 10\" :key=\"item\"></GoodsItem>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport GoodsItem from '@/components/GoodsItem.vue'\r\nimport { getHomeInfo } from '@/api/home'\r\nexport default {\r\n  name: 'HomePage',\r\n  data () {\r\n    return {\r\n      bannerList: [],\r\n      navList: [],\r\n      proList: []\r\n    }\r\n  },\r\n  components: {\r\n    GoodsItem\r\n  },\r\n  async created () {\r\n    const { data: { pageData } } = await getHomeInfo()\r\n    this.bannerList = pageData.items[1].data\r\n    this.navList = pageData.items[3].data\r\n    this.proList = pageData.items[6].data\r\n    console.log(pageData)\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 主题 padding\r\n.home {\r\n  padding-top: 100px;\r\n  padding-bottom: 50px;\r\n}\r\n\r\n// 导航条样式定制\r\n.van-nav-bar {\r\n  z-index: 999;\r\n  background-color: #c21401;\r\n  ::v-deep .van-nav-bar__title {\r\n    color: #fff;\r\n  }\r\n}\r\n\r\n// 搜索框样式定制\r\n.van-search {\r\n  position: fixed;\r\n  width: 100%;\r\n  top: 46px;\r\n  z-index: 999;\r\n}\r\n\r\n// 分类导航部分\r\n.my-swipe .van-swipe-item {\r\n  height: 185px;\r\n  color: #fff;\r\n  font-size: 20px;\r\n  text-align: center;\r\n  background-color: #39a9ed;\r\n}\r\n.my-swipe .van-swipe-item img {\r\n  width: 100%;\r\n  height: 185px;\r\n}\r\n\r\n// 主会场\r\n.main img {\r\n  display: block;\r\n  width: 100%;\r\n}\r\n\r\n// 猜你喜欢\r\n.guess .guess-title {\r\n  height: 40px;\r\n  line-height: 40px;\r\n  text-align: center;\r\n}\r\n\r\n// 商品样式\r\n.goods-list {\r\n  background-color: #f6f6f6;\r\n}\r\n</style>\r\n"],"mappings":"AAkDA,OAAAA,SAAA;AACA,SAAAC,WAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,OAAA;MACAC,OAAA;IACA;EACA;EACAC,UAAA;IACAP;EACA;EACA,MAAAQ,QAAA;IACA;MAAAL,IAAA;QAAAM;MAAA;IAAA,UAAAR,WAAA;IACA,KAAAG,UAAA,GAAAK,QAAA,CAAAC,KAAA,IAAAP,IAAA;IACA,KAAAE,OAAA,GAAAI,QAAA,CAAAC,KAAA,IAAAP,IAAA;IACA,KAAAG,OAAA,GAAAG,QAAA,CAAAC,KAAA,IAAAP,IAAA;IACAQ,OAAA,CAAAC,GAAA,CAAAH,QAAA;EACA;AACA"},"metadata":{},"sourceType":"